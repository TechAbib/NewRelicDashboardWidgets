{
  "manifestVersion": 1,
  "id": "new-relic-dashboard-widgets",
  "version": "0.2.1",
  "name": "New Relic Dashboard Widgets",
  "scopes": [ ],
  "description": "A collection of dashboard widgets that can be used to pull information from New Relic right into TFS/VSTS.",
  "publisher": "jonathan-mezach",
  "public": false,
  "icons": {
    "default": "img/logo.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "tags": [
    "Sample"
  ],
  "links": {
    "home": {
      "uri": "https://bit.ly"
    },
    "getstarted": {
      "uri": "https://bit.ly"
    },
    "learn": {
      "uri": "https://bit.ly"
    },
    "support": {
      "uri": "https://bit.ly"
    },
    "repository": {
      "uri": "https://bit.ly"
    },
    "issues": {
      "uri": "https://bit.ly"
    }
  },
  "branding": {
    "color": "rgb(220, 235, 252)",
    "theme": "light"
  },
  "files": [
    {
      "path": "css",
      "addressable": true
    },
    {
      "path": "img",
      "addressable": true
    },
    {
      "path": "scripts",
      "addressable": true
    },
    {
      "path": "server-status-widget.html",
      "addressable": true
    },
    {
      "path": "server-status-widget-configuration.html",
      "addressable":  true
    }
  ],
  "categories": [
    "Integrate"
  ],
  "contributions": [
    {
      "id": "ServerStatusWidget",
      "type": "ms.vss-dashboards-web.widget",
      "targets": [
        "ms.vss-dashboards-web.widget-catalog",
        "jonathan-mezach.new-relic-dashboard-widgets.ServerStatusWidget.Configuration"
      ],
      "properties": {
        "name": "Server Status Widget",
        "description": "Displays the New Relic status of a server.",
        "previewImageUrl": "img/preview.png",
        "uri": "server-status-widget.html",
        "supportedSizes": [
          {
            "rowSpan": 1,
            "columnSpan": 1
          }
        ],
        "supportedScopes": [ "project_team" ]
      }
    },
    {
      "id": "ServerStatusWidget.Configuration",
      "type": "ms.vss-dashboards-web.widget-configuration",
      "targets": [ "ms.vss-dashboards-web.widget-configuration" ],
      "properties": {
        "name": "ServerStatusWidget Configuration",
        "description": "Configures ServerStatusWidget",
        "uri": "server-status-widget-configuration.html"
      }
    }
  ]
}
